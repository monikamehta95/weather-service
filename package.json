{
  "name": "p3d-product-service",
  "version": "1.0.0",
  "description": "Platform 3D Product Service",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server/index.ts",
    "prod": "node ./src/server/index.js",
    "test": "jest",
    "coverage": "jest --coverage --forceExit --verbose --runInBand --detectOpenHandles",
    "watch-test": "yarn run test -- --watchAll",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run check",
    "migration_dev": "ts-node ./node_modules/typeorm/cli.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^3.0.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "csv-parser": "^2.3.2",
    "express": "^4.17.1",
    "helmet": "^3.20.1",
    "http-status-codes": "^1.3.2",
    "ip": "^1.1.5",
    "lodash": "^4.17.15",
    "makeerror": "^1.0.11",
    "mongoose": "^5.9.3",
    "oracledb": "^4.0.1",
    "pptxgenjs": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "supports-color": "6.1.0",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.1.1",
    "typeorm": "^0.2.22",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.44",
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.3",
    "@types/node": "^12.7.2",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^10.11.3",
    "dotenv": "^8.1.0",
    "gts": "^1.1.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^10.0.0-1",
    "node-mocks-http": "^1.8.1",
    "prettier": "1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.41",
    "typescript": "^3.5.3"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "clover.xml",
    "indent": 4
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run fix",
      "npm run check",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
